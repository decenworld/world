{
  "name": "world",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "mkdir -p public/dist && cp -r public/js public/dist/ && cp -r public/assets public/dist/ && cp public/index.html public/dist/",
    "clean": "rm -rf public/dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build"
  },
  "keywords": [
    "game",
    "multiplayer",
    "phaser",
    "socket.io"
  ],
  "author": "",
  "license": "ISC",
  "description": "A multiplayer game where you can move characters around and shoot",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
